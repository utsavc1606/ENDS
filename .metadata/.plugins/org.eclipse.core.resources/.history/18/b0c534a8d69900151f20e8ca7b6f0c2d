import hashlib
import collections

m = hashlib.md5()

inputFile = open("/Users/utsavchatterjee/Documents/Python Projects/ENDS Tweet Analysis/ENDS/Step2Op4Staging.txt", "r")
outputFile = open("/Users/utsavchatterjee/Documents/Python Projects/ENDS Tweet Analysis/ENDS/unwantedTweets.txt", "w")
tweetList = []
for line in inputFile:
    tweetText = line.split("-->")[1]
    tweetList.append(tweetText)

counter = collections.Counter(tweetList)
for items in counter.iteritems():
    if items[1] > 3:
        outputFile.write(items[0])





